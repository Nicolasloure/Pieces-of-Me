// persistent obj that will keep track of the vars below for Clock Puzzle
//
global.clock_puzzle_correct_answer = false

global.clock_puzzle_solved = false

if global.clock_puzzle_solved = true {
	
	
	global.clock_puzzle_hours_1 = 1
	global.clock_puzzle_hours_2 = 1

	global.clock_puzzle_minutes_1 = 1
	global.clock_puzzle_minutes_2 = 5
	}

	

else {
global.clock_puzzle_hours_1 = 0
global.clock_puzzle_hours_2 = 9

global.clock_puzzle_minutes_1 = 3
global.clock_puzzle_minutes_2 = 0
}


